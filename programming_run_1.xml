<steps>
    <step action='reset_gyro'/>
    <step action='launchInParallel' >
        <step action='Straight_gyro' speed='25' rotations='2.9'/>
        <step action='onForRotations' motor='mediumMotor' speed='-30' rotations='-0.05' gearRatio='1.4' />
    </step>
    <step action='launchInParallel' >
        <step action='tank_rotations' left_speed='35' right_speed='25' rotations='1.5' />
        <step action='onForRotations' motor='mediumMotor' speed='-30' rotations='-0.2' gearRatio='1.4'/>
    </step>

    <step action='delayForSeconds' seconds='1'/>
    <step action='tank_rotations' left_speed='-20' right_speed='-20' rotations='1' />
    <step action='turn_to_degrees' speed='20' degrees='0' direction='LEFT' />
    <step action='tank_rotations' left_speed='-20' right_speed='-20' rotations='8' />
</steps>
