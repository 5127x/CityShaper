    </step>
    <step action= 'squareOnLineWhite' speed = '4' target = '80'/>
    <step action = 'Tank_rotations' left_speed = '-5' right_speed = '-5' rotations = '.1' />
    <step action='Motor_onForRotations' motor='mediumMotor' speed='8' rotations='0.3' gearRatio='1.4' />
    <step action = 'Tank_rotations' left_speed = '-5' right_speed = '-5' rotations = '.5' />


    <step action='Do_nothing'/>


</steps>
