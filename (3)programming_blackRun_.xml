<steps>
    <!-- drive out -->
    <step action='Reset_gyro'/>
    <!-- place blocks -->
    <step action='Tank_rotations' left_speed = '20.8' right_speed = '20' rotations = '2' />
    <!-- back up, turn to home -->
    <step action='Tank_rotations' left_speed = '-15' right_speed = '-15' rotations = '.6' />
    <step action='Turn_degrees' speed = '15' degrees = '50'/>
    <!-- return to home -->
    <step action='Tank_rotations' left_speed = '40' right_speed = '50' rotations = '2' />
    <step action='Do_nothing' />
</steps>

